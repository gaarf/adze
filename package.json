{
  "name": "adze",
  "author": "adrien@continuuity.com",
  "license": "ISC",
  "version": "0.0.0",
  "description": "gulp less bootstrap angular",
  "main": "index.js",
  "scripts": {
    "postinstall": "bower install",
    "prestart": "npm install",
    "start": "gulp watch & http-server ./dist -p 8080",
    "pretest": "npm install",
    "test": "gulp build && ./node_modules/karma/bin/karma start test/karma.conf.js",
    "test-single-run": "gulp build && ./node_modules/karma/bin/karma start test/karma.conf.js --no-auto-watch --single-run",
    "preupdate-webdriver": "npm install",
    "update-webdriver": "webdriver-manager update",
    "preprotractor": "http-server -p 9090 ./dist & npm run update-webdriver",
    "protractor": "gulp build && protractor test/protractor-conf.js || true",
    "postprotractor": "pkill -fn \"http-server -p 9090\" && true"
  },
  "devDependencies": {
    "gulp": "^3.8.7",
    "gulp-angular-templatecache": "^1.3.0",
    "gulp-autoprefixer": "0.0.8",
    "gulp-concat": "^2.3.4",
    "gulp-if": "^1.2.4",
    "gulp-jshint": "^1.8.4",
    "gulp-less": "^1.3.3",
    "gulp-livereload": "^2.1.0",
    "gulp-load-plugins": "^0.5.3",
    "gulp-ng-annotate": "^0.3.0",
    "gulp-plumber": "^0.6.4",
    "gulp-rimraf": "^0.1.0",
    "gulp-size": "^1.0.0",
    "gulp-uglify": "^0.3.1",
    "gulp-util": "^3.0.0",
    "gulp-wrapper": "^0.1.5",
    "http-server": "^0.6.1",
    "karma": "^0.12.21",
    "karma-chrome-launcher": "^0.1.4",
    "karma-jasmine": "^0.1.5",
    "merge-stream": "^0.1.5",
    "protractor": "^1.0.0"
  }
}
